QLineEdit{
	border-top: 1px soild #e5e5e5;
	border-left: 1px soild #e5e5e5;
    border-right: 1px soild #e5e5e5;
    border-bottom: 1px solid #868686;; /* 边框宽度为1px，颜色为#A0A0A0 */
	border-radius: 3px; /* 边框圆角 */
	padding-left: 5px; /* 文本距离左边界有5px */
	background-color: #ffffff; /* 背景颜色 */
	color: #000000; /* 文本颜色 */
	selection-background-color: #1191d3; /* 选中文本的背景颜色 */
	selection-color: #F2F2F2; /* 选中文本的颜色 */
	font-family: "Microsoft YaHei"; /* 文本字体族 */
	font-size: 10pt; /* 文本字体大小 */
}
QLineEdit:hover,QLineEdit:editble,QLineEdit:focus{
border-bottom: 1px solid #1191d3;; /* 边框宽度为1px，颜色为#A0A0A0 */
}
